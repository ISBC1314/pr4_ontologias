    <owl:disjointWith rdf:resource="#Pato"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#Tigre"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class>
        <owl:complementOf rdf:resource="#Mascota"/>
      </owl:Class>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#EspecieAnimal"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Vegetariano"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Propietario">
