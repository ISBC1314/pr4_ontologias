        <tieneMascota rdf:resource="#Fluffy"/>
      </Persona>
    </es_tenida_por>
  </Pato>
  <Tigre rdf:ID="Titi"/>
  <Persona rdf:ID="Alfredo"/>
  <Gato rdf:ID="Misi">
    <come rdf:resource="#sesosPersona"/>
  </Gato>
