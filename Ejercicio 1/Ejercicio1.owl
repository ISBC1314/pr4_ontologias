            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#tieneMascota"/>
            </owl:onProperty>
          </owl:Restriction>
          <owl:Class rdf:about="#Propietario"/>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="Viejecita">
