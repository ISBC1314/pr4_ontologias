      <owl:Class rdf:ID="Comida"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="ComidaAnimal">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#Comida"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="PropietarioPerro">
