      <owl:Class rdf:about="#Vaca"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Tigre"/>
    </owl:disjointWith>
    <rdfs:subClassOf>
      <owl:Class rdf:about="#EspecieAnimal"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Persona">
    <owl:disjointWith>
      <owl:Class rdf:about="#Comida"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:about="#Animal"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="Mujer">
    <rdfs:subClassOf rdf:resource="#Persona"/>
  </owl:Class>
  <owl:Class rdf:about="#Animal">
    <owl:disjointWith rdf:resource="#Persona"/>
