      <owl:Class rdf:about="#Tigre"/>
    </owl:disjointWith>
    <rdfs:subClassOf rdf:resource="#EspecieAnimal"/>
  </owl:Class>
  <owl:Class rdf:ID="PropietarioGatos">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Propietario"/>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="#Gato"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#tieneMascota"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#Tigre">
    <owl:disjointWith rdf:resource="#Gato"/>
    <owl:disjointWith rdf:resource="#Perro"/>
    <owl:disjointWith rdf:resource="#Pato"/>
    <owl:disjointWith rdf:resource="#Vaca"/>
