      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#Gato"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:about="#tieneMascota"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="#Mujer"/>
  </owl:Class>
  <owl:Class rdf:about="#EspecieAnimal">
    <rdfs:subClassOf rdf:resource="#Animal"/>
  </owl:Class>
  <owl:Class rdf:about="#Vegetariano">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:someValuesFrom rdf:resource="#ComidaVegetal"/>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="come"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Animal"/>
          <owl:Restriction>
            <owl:allValuesFrom rdf:resource="#ComidaVegetal"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#come"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="VacaLoca">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Vaca"/>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="#SesosOveja"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#come"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:about="#Perro">
    <owl:disjointWith rdf:resource="#Gato"/>
    <owl:disjointWith rdf:resource="#Pato"/>
