      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#Persona"/>
          <owl:Restriction>
            <owl:someValuesFrom rdf:resource="#Animal"/>
            <owl:onProperty>
              <owl:ObjectProperty rdf:about="#tieneMascota"/>
            </owl:onProperty>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>
  </owl:Class>
  <owl:Class rdf:ID="AnimalLover">
    <owl:equivalentClass>
      <owl:Class>
        <owl:intersectionOf rdf:parseType="Collection">
          <owl:Restriction>
