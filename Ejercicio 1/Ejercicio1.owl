      <owl:Class rdf:ID="EspecieAnimal"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="Vegetariano"/>
    </rdfs:subClassOf>
    <owl:disjointWith>
      <owl:Class rdf:ID="Gato"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Perro"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="Vaca"/>
    </owl:disjointWith>
    <owl:disjointWith>
